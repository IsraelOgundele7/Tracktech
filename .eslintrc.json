{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-console" :"warn",
        "linebreak-style": ["error", "windows"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "no-unused-vars": "warn",
        "no-unused-expressions":"warn",
        "consistent-return": "warn",
        "eqeqeq": [1,"allow-null"]
    }
}